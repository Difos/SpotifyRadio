{
  "name": "spotifyradio",
  "version": "1.0.0",
  "description": "Seja bem vindo(a) à sexta Semana Javascript Expert. Este é o código inicial para iniciar nossa jornada.",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "node server/index.js",
    "live-reload": "nodemon server/index.js",
    "test": "set NODE_OPTIONS=--experimental-vm-modules && npx jest --runInBand --no-cache",
    "test:watch":"set NODE_OPTIONS=--experimental-vm-modules && npx jest --runInBand --no-cache --watchAll",
    "test:coverage":"set NODE_OPTIONS=--experimental-vm-modules && npx jest --runInBand --no-cache --coverage tests/unit",
    "live-reload:docker":"docker-compose up live-reload"
  },
  "engines": {
    "node": "14.x"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "nodemon": "^2.0.15"
  },
  "dependencies": {
    "@types/jest": "^27.4.1",
    "jest": "^27.5.1",
    "pino": "^7.8.0",
    "pino-pretty": "^7.5.3"
  }
}
